							"%%pyspark\r\n",
							"df = spark.read.load('abfss://raw@dlssynapserooz.dfs.core.windows.net/disclose_list_of_offers/7feb971e-804f-4aa6-9c0e-7303e6f0e2e9.json', format='json')"
