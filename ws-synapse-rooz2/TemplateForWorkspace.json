							",col(\"offers.get:FilingType\").alias(\"filing_type\")\r\n",
							",col(\"offers.get:EntityType\").alias(\"entity_type\")\r\n",
							",col(\"offers.get:VersionDate\").alias(\"version_date\")\r\n",
							",col(\"offers.get:VersionNumber\").alias(\"version_number\")\r\n",
							"\r\n",
